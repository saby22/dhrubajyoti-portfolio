<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%=title%></title>

    <!-- Stylesheets -->
    <% if(process.env.ELEVENTY_ENV.trim() === 'production') {%> 
        <% for(file of data.css) %> 
            <link rel="stylesheet" href="./static/<%= file %>">
    <% } %> 

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Rubik:wght@400;500;700&amp;display=swap&amp;text=DHRUBAJYOTIBISWASRESEARCHSCHOLAR@IITMADRASABOUTMEGETCV" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Be+Vietnam:wght@300;400;600;700;800&display=swap" rel="stylesheet">

    <!-- Academicons -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/jpswalsh/academicons@1/css/academicons.min.css">
    
    <!-- Favicons -->
    <link rel="apple-touch-icon" sizes="180x180" href="https://dhrubajyoti98.netlify.app/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="https://dhrubajyoti98.netlify.app/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="https://dhrubajyoti98.netlify.app/favicons/favicon-16x16.png">
    <link rel="manifest" href="https://dhrubajyoti98.netlify.app/favicons/site.webmanifest">
