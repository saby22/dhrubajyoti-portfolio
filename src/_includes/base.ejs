    <meta name="msapplication-TileColor" content="#231f20">
    <meta name="msapplication-config" content="https://dhrubajyoti98.netlify.app/favicons/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">
    
</head>
<body>
    
    <!-- Fontawesome -->
    <script src="https://kit.fontawesome.com/befeaae48c.js" crossorigin="anonymous" defer></script>
    
    <!-- Script -->
    <% if(process.env.ELEVENTY_ENV.trim() === 'developement') {%> 
        <script src="./assets/index.js" defer></script>
    <% } else {%> 
        <% for(file of data.js){ %> 
            <script src="./static/<%= file %> " defer></script>
        <% } }%> 


    <%- content %> 

</body>
</html>