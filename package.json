{
  "name": "dhrubajyoti98-portfolio",
  "version": "1.0.0",
  "description": "Portfolio site generated using Eleventy for Dhrubajyoti Biswas",
  "main": "index.js",
  "scripts": {
    "build:site": "SET ELEVENTY_ENV=production && npx @11ty/eleventy",
    "dev:site": "SET ELEVENTY_ENV=developement && npx @11ty/eleventy --serve --watch",
    "build:assets": "webpack --config webpack.config.prod.js",
    "dev:assets": "SET ELEVENTY_ENV=developement && webpack --config webpack.config.dev.js",
    "build": "export ELEVENTY_ENV=production && webpack --config webpack.config.prod.js && npx @11ty/eleventy"
  },
  "author": "Sabyasachi",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "0.11.1",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "1.3.3",
    "postcss": "^8.2.3",
    "postcss-loader": "^4.1.0",
    "sass": "1.32.2",
    "sass-loader": "10.1.0",
    "webpack": "5.11.1",
    "webpack-cli": "4.3.1",
    "webpack-merge": "5.7.3"
  },
  "dependencies": {
    "autoprefixer": "^10.2.1",
    "axios": "0.21.1",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "8.2.0",
    "html-minifier": "^4.0.0",
    "prismic-dom": "2.2.4",
    "resolve-url-loader": "^3.1.2",
    "style-loader": "^2.0.0"
  }
}
